                android:layout_height="50dp"
                android:layout_below="@+id/delete1"
                android:onClick="delete"
                android:text="delete"
                android:visibility="invisible"
                android:layout_alignLeft="@+id/delete3"
                android:layout_alignStart="@+id/delete3" />

            <Button
